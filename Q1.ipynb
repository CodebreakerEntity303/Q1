    "#Q1\n",
    "def moveZeros(nums):\n",
    "    i = 0 \n",
    "    for j in range(len(nums)):\n",
    "        if nums[j] != 0:\n",
    "            nums[i], nums[j] = nums[j], nums[i]\n",
    "            i += 1 \n",
    "nums = [0, 1, 0, 3, 12]\n",
    "moveZeros(nums)\n",
    "print(nums)\n",
    "nums = [0]\n",
    "moveZeros(nums)\n",
    "print(nums)"
   ]
